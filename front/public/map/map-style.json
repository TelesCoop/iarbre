{
  "CADASTRE": {
    "glyphs": "https://fonts.openmaptiles.org/{fontstack}/{range}.pbf",
    "layers": [
      {
        "id": "osm",
        "source": "osm",
        "type": "raster"
      },
      {
        "id": "cadastre-layer",
        "paint": {
          "fill-color": "#ffffff",
          "fill-opacity": 0,
          "fill-outline-color": "#ffffff"
        },
        "source": "cadastre",
        "source-layer": "cadastre--cadastre",
        "type": "fill"
      },
      {
        "id": "cadastre-border",
        "paint": {
          "line-color": "#ffffff",
          "line-width": 2
        },
        "source": "cadastre",
        "source-layer": "cadastre--cadastre",
        "type": "line"
      },
      {
        "id": "cadastre-labels",
        "layout": {
          "text-allow-overlap": false,
          "text-anchor": "center",
          "text-field": [
            "number-format",
            [
              "to-number",
              [
                "slice",
                [
                  "get",
                  "parcel_id"
                ],
                -4
              ]
            ],
            {}
          ],
          "text-font": [
            "Open Sans Regular"
          ],
          "text-ignore-placement": false,
          "text-size": 14
        },
        "paint": {
          "text-color": "#000000",
          "text-halo-color": "#ffffff",
          "text-halo-width": 1
        },
        "source": "cadastre",
        "source-layer": "cadastre--cadastre",
        "type": "symbol"
      }
    ],
    "sources": {
      "cadastre": {
        "attribution": "Cadastre &copy; Etalab",
        "tiles": [
          "{API_BASE_URL}/tiles/cadastre/cadastre/{z}/{x}/{y}.mvt"
        ],
        "type": "vector"
      },
      "osm": {
        "attribution": "&copy; OpenStreetMap Contributors",
        "tileSize": 256,
        "tiles": [
          "https://a.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png",
          "https://b.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png",
          "https://c.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"
        ],
        "type": "raster"
      }
    },
    "version": 8
  },
  "OSM": {
    "layers": [
      {
        "id": "osm",
        "source": "osm",
        "type": "raster"
      }
    ],
    "sources": {
      "osm": {
        "attribution": "&copy; OpenStreetMap Contributors",
        "tileSize": 256,
        "tiles": [
          "https://a.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png",
          "https://b.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png",
          "https://c.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"
        ],
        "type": "raster"
      }
    },
    "version": 8
  },
  "SATELLITE": {
    "layers": [
      {
        "id": "satellite-layer",
        "source": "satellite",
        "type": "raster"
      }
    ],
    "sources": {
      "satellite": {
        "attribution": "Imagery &copy; Esri",
        "tileSize": 256,
        "tiles": [
          "https://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
        ],
        "type": "raster"
      }
    },
    "version": 8
  }
}
