{
  "name": "iarbre-front",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview --port 4173",
    "test": "vitest tests",
    "test:unit": "vitest",
    "test:cypress": "CYPRESS_COVERAGE=true NODE_ENV=test start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress run'",
    "test:cypress:manually": "NODE_ENV=test start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@maplibre/maplibre-gl-geocoder": "^1.8.0",
    "@tailwindcss/vite": "^4.0.17",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^14.0.0",
    "d3": "^7.9.0",
    "driver.js": "^1.4.0",
    "jsdom": "^26.1.0",
    "maplibre-gl": "^5.4.0",
    "pinia": "^3.0.0",
    "polished": "^4.3.1",
    "strip-ansi": "^7.1.0",
    "terra-draw": "^1.19.0",
    "terra-draw-maplibre-gl-adapter": "^1.2.2",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@cypress/code-coverage": "^3.13.8",
    "@lavamoat/allow-scripts": "^3.4.1",
    "@pinia/testing": "^1.0.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/d3": "^7.4.3",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "^13.12.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-eslint-plugin": "^6.3.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.5.3",
    "postcss-for": "^2.1.1",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^4.0.17",
    "typescript": "5.6.3",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^5.4.21",
    "vite-plugin-istanbul": "^7.1.0",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.0.21"
  },
  "lavamoat": {
    "allowScripts": {
      "cypress": true,
      "vite>esbuild": true
    }
  }
}
